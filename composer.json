{
  "name": "keopiwauyu/hyundaicommando",
  "type": "project",
  "license": "Apache-2.0",
  "require": {
    "pocketmine/pocketmine-mp": "^4",
    "cortexpe/commando": "dev-master",
    "sylvrs/libmarshal": "^1.4",
    "sof3/await-generator": "^3.4",
    "sof3/await-std": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "keopiwauyu\\HyundaiCommando\\": "src"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "1.4.6",
    "phpstan/phpstan-strict-rules": "^1.0",
    "phpstan/extension-installer": "^1.0"
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
